{
    "include": [
      "./src/**/*",
      "./gatsby-*.ts"
    ],
    "compilerOptions": {
      "target": "es2019",
      "module": "commonjs",
      "lib": ["dom", "es2017"],
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
        "graphql-types": [
          "types/graphql-types",
          "types/graphql-types-stub"
        ]
      }
    }
}